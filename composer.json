{
    "name": "dcblogdev/phpcart",
    "description": "Simple framework agnostic shopping cart",
    "keywords": [
        "Cart",
        "php cart",
        "Shopping cart",
        "Laravel shopping cart",
        "Laravel cart"
    ],
    "homepage": "https://github.com/dcblogdev/phpcart",
    "license": "MIT",
    "authors": [
        {
            "name": "Anam hossain",
            "email": "enam33@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x|6.x|7.x|8.x"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dcblogdev\\Phpcart\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcblogdev\\Phpcart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcblogdev\\Phpcart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Dcblogdev\\Phpcart\\Facades\\Cart"
            }
        }
    }
}
